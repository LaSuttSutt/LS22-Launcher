<Window x:Class="LS22_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LS22_Launcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Window.Resources>
        <ResourceDictionary Source="Templates/ResourceDictionaryTemplate.xaml" />
    </Window.Resources>
    <Grid>
        <TabControl TabStripPlacement="Left" Margin="5">
            <TabItem Template="{StaticResource TabItem}" Header="Test" />
            <TabItem Template="{StaticResource TabItem}" Header="Test2" />
            <TabItem Template="{StaticResource TabItem}" Header="Test3" />
        </TabControl>
    </Grid>
</Window>
